<h3>Demo Chart 1</h3>
<div style="height:400px; width:400px">
    <canvas id="TestChart"></canvas>
</div>


<script>

    var x = @Html.Raw(Json.Serialize(ViewBag.Data.Item1));
    var y = @Html.Raw(Json.Serialize(ViewBag.Data.Item2));
    var can = document.getElementById("TestChart");
    var chart = new Chart(can, {
        type: 'doughnut',

        data: {
            labels: ["Male", "Female"],
            datasets: [
                {
                    label: "Label i think",
                    data:[x,y]
                }
            ]
        },

        options: {}
    });
</script>